{
  "name": "sora-backend",
  "version": "1.0.0",
  "description": "",
  "main": "./build/index.js",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx watch ./src/index.ts",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "build": "tsc",
    "test": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest",
    "test:watch": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" jest --watchAll",
    "type-check": "tsc --noEmit",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "prepare": "husky install",
    "db:drop": "dotenv -e .env.local -- cross-env-shell \"echo DROP DATABASE IF EXISTS $DATABASE_TO_DROP\" | dotenv -e .env.local -- prisma db execute --stdin",
    "db:seed": "dotenv -e .env.local -- prisma db seed",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:reset": "dotenv -e .env.local -- prisma migrate reset --force",
    "db:create": "dotenv -e .env.local -- cross-env-shell \"echo CREATE DATABASE $DATABASE_TO_CREATE\" | dotenv -e .env.local -- prisma db execute --stdin",
    "db:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:migrate:create-only": "dotenv -e .env.local -- prisma migrate dev --create-only"
  },
  "prisma": {
    "schema": "src/utils/prisma/schema.prisma",
    "seed": "tsx ./src/utils/prisma/seed.ts"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "engines": {
    "node": "20.15.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^6.4.15",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "globals": "^15.8.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "3.1.0",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.3.1",
    "@googleapis/oauth2": "^1.0.7",
    "@prisma/client": "^5.16.0",
    "bcrypt": "^5.1.1",
    "cloudinary": "^1.41.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^140.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "prisma": "^5.16.0",
    "ts-jest": "^29.1.5",
    "validator": "^13.11.0"
  }
}
